{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Pico RP2040 Debug",
            "cwd": "${workspaceFolder}/debug/scripts",
            "executable": "${command:cmake.launchTargetPath}",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "serverpath": "${workspaceFolder}/debug/openocd.exe",
            "gdbPath": "arm-none-eabi-gdb",
            "device": "RP2040",
            "configFiles": [
                "${workspaceFolder}/debug/scripts/interface/cmsis-dap.cfg",
                "${workspaceFolder}/debug/scripts/target/rp2040.cfg"
            ],
            "svdFile": "${workspaceFolder}/external/pico-sdk/src/rp2040/hardware_regs/RP2040.svd",
            "runToEntryPoint": "main",
            "preLaunchTask": "Compile Project",
            "overrideLaunchCommands": [
                "monitor reset init",
                "load ../../build/ogx-mini.elf"
            ],
            "openOCDLaunchCommands": [
                "adapter speed 5000"
            ]
        },
        {
            "name": "Pico RP2350 Debug",
            "cwd": "${workspaceFolder}/debug/scripts",
            "executable": "${command:cmake.launchTargetPath}",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "serverpath": "${workspaceFolder}/debug/openocd.exe",
            "gdbPath": "arm-none-eabi-gdb",
            "device": "RP2350",
            "configFiles": [
                "${workspaceFolder}/debug/scripts/interface/cmsis-dap.cfg",
                "${workspaceFolder}/debug/scripts/target/rp2350.cfg"
            ],
            "svdFile": "${workspaceFolder}/external/pico-sdk/src/rp2350/hardware_regs/RP2350.svd",
            "runToEntryPoint": "main",
            "preLaunchTask": "Compile Project",
            "overrideLaunchCommands": [
                "monitor reset init",
                "load ../../build/ogx-mini.elf"
            ],
            "openOCDLaunchCommands": [
                "adapter speed 5000"
            ]
        }
    ]
}